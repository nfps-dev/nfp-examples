{
  "name": "@nfps.dev/example-battleship",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "rollup": "./node_modules/.bin/rollup",
    "vite": "./node_modules/.bin/vite",
    "build:contract:interface": "contractor rust -i contract/interface/app.ts -o contract/src/msgs.rs -f",
    "build:bootloader": "yarn rollup -c bootloader/rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev:bootloader": "yarn build:bootloader --environment NFP_ENV:development",
    "build:svg": "node build.mjs",
    "dev:svg": "NFP_ENV=development yarn build:svg -la -o dist/nfp.dev.svg",
    "build:nfp": "yarn build:bootloader && yarn build:svg",
    "dev:nfp": "yarn dev:bootloader && yarn dev:svg",
    "build:app": "yarn vite build app",
    "dev:app": "NFP_ENV=development yarn build:app --mode development",
    "watch:app": "yarn dev:app -w",
    "deploy:app": "nfp package upload dist/app.js",
    "build:applibs": "yarn rollup -c applibs/rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev:applibs": "yarn build:applibs --environment NFP_ENV:development",
    "build": "yarn build:applibs && yarn build:app && yarn build:nfp",
    "dev": "yarn dev:applibs && yarn dev:app && yarn dev:nfp && echo \"\\n-------- READY --------\\n\"",
    "watch:dev": "ls {app,applibs,bootloader,media}/**/*.* package.json build.{mjs,ts} | entr yarn dev",
    "watch:build": "ls {app,applibs,bootloader,media}/**/*.* package.json build.{mjs,ts} | entr yarn build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@blake.regalia/belt": "^0.15.0",
    "@blake.regalia/eslint-config-elite": "^0.3.3",
    "@blake.regalia/tsconfig": "^0.1.2",
    "@cosmjs/amino": "^0.31.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@keplr-wallet/types": "^0.12.28",
    "@nfps.dev/rollup-plugin-microweb": "^0.3.5",
    "@nfps.dev/sdk": "^0.7.7",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.4",
    "@solar-republic/contractor": "^0.4.1",
    "@types/node": "^20.7.2",
    "@types/web": "^0.0.115",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/parser": "^6.7.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "io-ts-transformer": "^1.1.8",
    "less": "^4.2.0",
    "rollup": "^3.29.4",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "ws": "^8.14.2",
    "xml-formatter": "^3.5.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@nfps.dev/runtime": "^0.13.1",
    "@solar-republic/neutrino": "^0.9.3",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "idb-keyval": "^6.2.1",
    "pixi.js": "^7.3.1"
  }
}
